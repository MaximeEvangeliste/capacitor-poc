{
  "name": "capacitor-poc",
  "private": true,
  "scripts": {
    "build:mobile": "next build && next export && cap sync",
    "build:web": "next build",
    "dev": "cap sync && next dev",
    "export": "next export",
    "i18n": "NODE_ENV=development yarn lingui extract",
    "lint": "next lint",
    "open:android": "cap open android",
    "open:ios": "cap open ios",
    "reinstall": "yarn install && cap sync",
    "remove-package": "sh -c 'yarn remove ${0} && cap sync'",
    "run:android": "cap sync && cap run android",
    "run:ios": "cap sync && cap run ios",
    "set-local-ip": "node ./set-local-ip.js && cap sync",
    "start": "next start"
  },
  "dependencies": {
    "@capacitor/android": "^3.3.4",
    "@capacitor/core": "^3.3.4",
    "@capacitor/device": "^1.1.1",
    "@capacitor/dialog": "^1.0.6",
    "@capacitor/geolocation": "^1.3.0",
    "@capacitor/ios": "^3.3.4",
    "@lingui/react": "^3.13.0",
    "capacitor-plugin-safe-area": "^0.0.3",
    "faker": "5.5.3",
    "local-ip": "^2.0.0",
    "make-plural": "^7.0.0",
    "next": "^12.0.8-canary.22",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "zustand": "^3.6.9"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@capacitor/cli": "^3.3.4",
    "@lingui/cli": "^3.13.0",
    "@lingui/loader": "^3.13.0",
    "@lingui/macro": "^3.13.0",
    "@types/faker": "^5.5.9",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.11",
    "typescript": "4.5.4"
  }
}
